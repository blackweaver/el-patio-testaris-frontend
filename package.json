        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~2.18.8",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.13",
    "bstyles": "^0.3.1",
    "chalk": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-changed": "^3.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-confirm": "^1.0.5",
    "gulp-data": "^1.2.1",
    "gulp-debug": "^3.1.0",
    "gulp-filesize": "0.0.6",
    "gulp-filter": "^5.0.0",
    "gulp-git": "^2.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-minify-css": "~1.2.4",
    "gulp-notify": "^3.0.0",
    "gulp-rename": "^1.2.2",
